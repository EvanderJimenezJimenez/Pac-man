<AnchorPane id="AnchorPane" prefHeight="551.0" prefWidth="684.0" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cr.ac.una.pac.man.controller.ChooseLevelController">
